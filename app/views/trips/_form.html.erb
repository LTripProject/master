<div class="container">
  <%= simple_form_for(@trip) do |f| %>
    <%= f.error_notification %>

    <div class="form-inputs">
      <%= f.input :start_date %>
      <%= f.input :title %>
      <%= f.input :description %>
      <%= f.input :expected_budget %>
    </div>

    <div class="form-actions">
      <%= f.button :submit %>
    </div>
<% end %>

</div>`